{
  "training": {
    "trainer": "lightning",
    "prefetch_factor": 200,
    "num_workers": 1,
    "checkpoint_interval": 1000,
    "batch_size": 128
  },
  "optimizer": {
    "params": {
      "lr": "1e-4",
      "weight_decay": 0
    },
    "type": "adam_w"
  },
  "scheduler": {
    "interval": "step",
    "params": {
      "warmup_steps": 9766,
      "max_lr": "1e-4"
    }
  },
  "trainer": {
    "params": {
      "accelerator": "gpu",
      "devices": 1,
      "num_nodes": 64,
      "precision": "bf16",
      "accumulate_grad_batches": 8,
      "gradient_clip_val": 1
    }
  },
  "dataset": {
    "rnaseq": {
      "bin_num": 100,
      "bin_type": "auto_bin",
      "value_mask_prob": 0.3,
      "zero_mask_prob": 0.03,
      "replace_prob": 0.8,
      "random_token_prob": 0.1
    }
  }
}